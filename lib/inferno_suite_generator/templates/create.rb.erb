require 'inferno_suite_generator/test_modules/create_test'

module <%= suite_module_name %>
  module <%= module_name %>
    class <%= class_name %> < Inferno::Test
      include InfernoSuiteGenerator::CreateTest

      title '(<%= conformance_expectation %>) Server returns correct <%= resource_type %> resource from <%= resource_type %> create interaction'
      description 'A server <%= conformance_expectation %> support the <%= resource_type %> create interaction.'

      id :<%= test_id %>

        def resource_type
          '<%= resource_type %>'
        end

      def input_data
        <%= create_input_data[:default] %>
      end

      run do
        perform_create_test
      end
    end
  end
end
