{
  "metadata": {
    "description": "Configuration for Australian FHIR Core profiles testing",
    "last_updated": "2025-07-24",
    "version": "1.0.0"
  },
  "id": "hl7.fhir.au.core",
  "version": "2.0.0-draft",
  "title": "AU Core",
  "suite_module_name": "AUCoreTestKit",
  "module_name_prefix": "AUCore",
  "test_id_prefix": "au_core",
  "cs_profile_url": "http://hl7.org.au/fhir/core/CapabilityStatement/au-core-responder",
  "cs_version_specific_url": "https://hl7.org.au/fhir/core/2.0.0-draft/CapabilityStatement-au-core-responder.html",
  "default_fhir_server": "https://fhir.hl7.org.au/aucore/fhir/DEFAULT",
  "ig_link": "https://hl7.org.au/fhir/core/2.0.0-draft/index.html",
  "ig_name": "AU Core Implementation Guide",
  "tx_server_url": "https://tx.dev.hl7.org.au/fhir",
  "paths": {
    "ig_deps": "lib/au_core_test_kit/igs/",
    "result_folder": "./lib/au_core_test_kit/generated/",
    "related_result_folder": "/lib/au_core_test_kit/generated/",
    "ig_packages": "lib/au_core_test_kit/igs/*.tgz",
    "main_file": "lib/au_core_test_kit.rb",
    "ig_output_directory": "lib/au_core_test_kit/igs",
    "ig_json_files": "lib/au_core_test_kit/igs/*.json",
    "extra_json_paths": [
      "search-params.json"
    ]
  },
  "outer_groups": [
    {
      "import_type": "relative",
      "import_path": "../../custom_groups/v0.3.0-ballot/capability_statement_group",
      "group_position": "before",
      "group_id": "au_core_v030_ballot_capability_statement"
    },
    {
      "import_type": "relative",
      "import_path": "../../custom_groups/missing_data_group",
      "group_position": "after",
      "group_id": "au_core_missing_data_group"
    }
  ],
  "links": [
    {
      "label": "Report Issue",
      "url": "https://github.com/hl7au/au-fhir-core-inferno/issues"
    },
    {
      "label": "Open Source",
      "url": "https://github.com/hl7au/au-fhir-core-inferno"
    },
    {
      "label": "AU Core Implementation Guide",
      "url": "https://build.fhir.org/ig/hl7au/au-fhir-core/"
    }
  ],
  "configs": {
    "JURISDICTION_FILTER": {
      "_description": "Configuration for jurisdiction filtering in search parameters",
      "system": "urn:iso:std:iso:3166",
      "code": "AU"
    },
    "multipleOrExpectations": {
      "_comment": "Hard-coded values are used because the multipleOr attributes do not exist in the machine-readable files, but they do exist in the narrative. (https://github.com/hl7au/au-fhir-core-inferno/issues/61, https://github.com/hl7au/au-fhir-core-inferno/issues/63)",
      "Procedure": {
        "Procedure-status": "SHALL",
        "clinical-code": "SHOULD"
      },
      "Observation": {
        "Observation-status": "SHALL",
        "clinical-code": "SHOULD"
      },
      "MedicationRequest": {
        "medications-status": "SHALL",
        "MedicationRequest-intent": "SHOULD"
      },
      "Immunization": {
        "Immunization-vaccine-code": "SHOULD"
      },
      "Condition": {
        "clinical-code": "MAY"
      },
      "Encounter": {
        "Encounter-status": "MAY"
      }
    },
    "multipleAndExpectations": {
      "_comment": "Hard-coded values are used because the multipleAnd attributes do not exist in the machine-readable files, but they do exist in the narrative. (https://github.com/hl7au/au-fhir-core-inferno/issues/62)",
      "Observation": {
        "clinical-date": "SHOULD"
      },
      "Condition": {
        "Condition-onset-date": "SHOULD"
      },
      "Encounter": {
        "clinical-date": "SHOULD"
      },
      "Immunization": {
        "clinical-date": "SHOULD"
      },
      "MedicationRequest": {
        "MedicationRequest-authoredon": "SHOULD"
      },
      "Patient": {
        "individual-birthdate": "MAY"
      },
      "Procedure": {
        "clinical-date": "MAY"
      }
    },
    "FIXED_SEARCH_VALUES": {
      "_description": "In the current step we don't need to check the correct content of the response. We should care about the correct structure of the request. In this current case we use dates just to check that server can make a response for the request.",
      "values": {
        "diagnostic_result": ["251739003", "24701-5"],
        "date": ["ge1950-01-01", "le2050-01-01", "gt1950-01-01", "lt2050-01-01"],
        "datetime": ["ge1950-01-01T00:00:00.000Z", "le2050-01-01T23:59:59.999Z", "gt1950-01-01T00:00:00.000Z", "lt2050-01-01T23:59:59.999Z"]
      },
      "resource_mappings": {
        "Observation": {
          "clinical-date": "datetime",
          "clinical-code": {
            "value": "diagnostic_result",
            "condition": {
              "profile_url": "http://hl7.org.au/fhir/core/StructureDefinition/au-core-diagnosticresult"
            }
          }
        },
        "Condition": {
          "Condition-onset-date": "datetime"
        },
        "Encounter": {
          "clinical-date": "datetime"
        },
        "Immunization": {
          "clinical-date": "datetime"
        },
        "MedicationRequest": {
          "MedicationRequest-authoredon": "datetime"
        },
        "Patient": {
          "individual-birthdate": "date"
        },
        "Procedure": {
          "clinical-date": "datetime"
        }
      }
    },
    "NAMING": {
      "_description": "Maps FHIR structure definition URLs to constant names used in the codebase",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-allergyintolerance": "ALLERGYINTOLERANCE",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-bloodpressure": "BLOOD_PRESSURE",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-bodyheight": "BODY_HEIGHT",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-bodytemp": "BODY_TEMPERATURE",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-bodyweight": "BODY_WEIGHT",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-condition": "CONDITION",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-diagnosticresult": "DIAGNOSTIC_RESULT_OBSERVATION",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-diagnosticresult-path": "PATHOLOGY_RESULT_OBSERVATION",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-encounter": "ENCOUNTER",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-heartrate": "HEART_RATE",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-immunization": "IMMUNIZATION",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-location": "LOCATION",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-medication": "MEDICATION",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-medicationrequest": "MEDICATIONREQUEST",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-medicationstatement": "MEDICATIONSTATEMENT",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-organization": "ORGANIZATION",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-patient": "PATIENT",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-practitioner": "PRACTITIONER",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-practitionerrole": "PRACTITIONERROLE",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-procedure": "PROCEDURE",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-provenance": "PROVENANCE",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-resprate": "RESPIRATION_RATE",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-rsg-sexassignedab": "SEX_ASSIGNED_AT_BIRTH",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-smokingstatus": "SMOKING_STATUS",
      "http://hl7.org.au/fhir/core/StructureDefinition/au-core-waistcircum": "WAIST_CIRCUMFERENCE"
    },
    "SKIP_PROFILES": {
      "_description": "Profiles that should be skipped during testing",
      "profiles": [
        "http://hl7.org.au/fhir/StructureDefinition/au-specimen",
        "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire",
        "http://hl7.org.au/fhir/core/StructureDefinition/au-core-norelevantfinding",
        "http://hl7.org/fhir/StructureDefinition/DocumentReference"
      ]
    },
    "SPECIAL_CASES": {
      "_description": "Special handling for specific profiles during testing",
      "READ_TEST_IDS_INPUTS": {
        "_description": "",
        "Location": {
          "input_id": "location_ids",
          "title": "Location IDs",
          "description": "Comma separated list of location IDs that in sum contain all MUST SUPPORT elements",
          "default": "bobrester-medical-center, au-hospital"
        },
        "Organization": {
          "input_id": "organization_ids",
          "title": "Organization IDs",
          "description": "Comma separated list of organization IDs that in sum contain all MUST SUPPORT elements",
          "default": "dva-au, organization-medical-center-tc"
        },
        "Practitioner": {
          "input_id": "practitioner_ids",
          "title": "Practitioner IDs",
          "description": "Comma separated list of practitioner IDs that in sum contain all MUST SUPPORT elements",
          "default": "alderson-helene"
        },
        "PractitionerRole": {
          "input_id": "practitioner_role_ids",
          "title": "PractitionerRole IDs",
          "description": "Comma separated list of practitioner role IDs that in sum contain all MUST SUPPORT elements",
          "default": "cardiologist-sallie-sutherland, bobrester-bob-gp"
        },
        "HealthcareService": {
          "input_id": "healthcare_service_ids",
          "title": "HealthcareService IDs",
          "description": "Comma separated list of healthcare services IDs that in sum contain all MUST SUPPORT elements",
          "default": "generalpractice-bungabbee-medical-clinic, communityhealth-southedge-practice, dentalpractice-annandale-dental, pathologylaboratory-woodcroft-pathology"
        }
      },
      "COMPARATORS": {
        "_description": "Hard-coded values are used because the comparator expectation does not exist in the machine-readable files, but it does exist in the narrative. (https://github.com/hl7au/au-fhir-core-inferno/issues/48)",
        "resources": [
          "Observation",
          "Condition",
          "Encounter",
          "Immunization",
          "MedicationRequest",
          "Patient",
          "Procedure"
        ],
        "operators": ["gt", "lt", "ge", "le"],
        "param_ids": [
          "clinical-date",
          "Condition-onset-date",
          "clinical-date",
          "MedicationRequest-authoredon",
          "individual-birthdate"
        ]
      },
      "RESOURCES_TO_EXCLUDE": {
        "_description": "",
        "resources": ["Medication", "DiagnosticReport"]
      },
      "VERSION_SPECIFIC_RESOURCES_TO_EXCLUDE": {
        "v1.0.0": ["Medication", "RelatedPerson"]
      },
      "PROFILES_TO_EXCLUDE": [],
